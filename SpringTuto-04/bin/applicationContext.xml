<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 
 <bean id="HelloWorld" class="com.aayu.pkg.HelloWorld">
 
 <property name="message1" value="Message one Hello World !"></property>
 <property name="message2" value="Message two Hello World!"></property>
 
 </bean>
 <bean id="HelloIndia" class="com.aayu.pkg.HelloIndia" parent="HelloWorld">

  <property name="message3" value="Message third HelloIndia!"></property> 
 </bean>
 </beans> 